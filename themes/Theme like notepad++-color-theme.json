{
	"name": "Theme like notepad++",
	"type": "light",
	"colors": {
		"editor.background": "#e1e1e1",
		"editor.foreground": "#111111",
		"sideBar.background": "#f0f0f0",
		"list.activeSelectionBackground": "#9e0707",
		"list.hoverBackground": "#b1bce4",
		"terminal.background": "#000",
		"terminal.foreground": "#eeeeee"
	},
	"tokenColors": [
		{
			"settings": {
				"foreground": "#ff0000",
				"background": "#f5f5f5ff"
			}
		},
		{
			"name": "php tag",
			"scope": [
				"punctuation.section.embedded.begin.php",
				"punctuation.section.embedded.end.php"
			],
			"settings": {
				"fontStyle": "normal",
				"foreground": "#ff0000"
			}
		},
		{
			"name": "php string quoted single",
			"scope": [
				"string.quoted.single.php"
			],
			"settings": {
				"fontStyle": "normal"
			}
		},
		{
			"name": "php array",
			"scope": [
				"punctuation.section.array.begin.php",
				"punctuation.section.array.end.php"
			],
			"settings": {
				"fontStyle": "normal"
			}
		},
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#AAAAAA"
			}
		},
		{
			"name": "Comments: Preprocessor",
			"scope": "comment.block.preprocessor",
			"settings": {
				"fontStyle": "",
				"foreground": "#AAAAAA"
			}
		},
		{
			"name": "Comments: Documentation",
			"scope": [
				"comment.documentation",
				"comment.block.documentation",
				"comment.line.double-slash.php",
				"comment.line.double-slash.js",
				"comment.block.js",
				"comment.block.html",
				"comment",
				"punctuation.definition.comment",
				"invalid.illegal.bad-comments-or-CDATA.html"
			],
			"settings": {
				"foreground": "#448C27"
			}
		},
		{
			"name": "Invalid - Deprecated",
			"scope": "invalid.deprecated",
			"settings": {
				"background": "#96000014"
			}
		},
		{
			"name": "Invalid - Illegal",
			"scope": "invalid.illegal",
			"settings": {
				"background": "#96000014",
				"foreground": "#660000"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage"
			],
			"settings": {
				"foreground": "#003399"
			}
		},
		{
			"name": "Types",
			"scope": [
				"storage.type",
				"support.type"
			],
			"settings": {
				"foreground": "#7A3E9D"
			}
		},
		{
			"name": "Language Constants",
			"scope": [
				"constant.language",
				"support.constant",
				"variable.language"
			],
			"settings": {
				"foreground": "#AB6526"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"support.variable"
